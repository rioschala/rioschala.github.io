    "@types/node": "^24.10.1",
    "autoprefixer": "^10.4.21",
    "pagefind": "^1.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "reading-time": "^1.5.0",
    "remark-unwrap-images": "^4.0.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3"
