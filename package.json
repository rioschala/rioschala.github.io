{
  "name": "astro-cactus",
  "version": "4.11.0",
  "private": true,
  "scripts": {
    "dev": "TINA_PUBLIC_IS_LOCAL=true tinacms dev -c \"astro dev\"",
    "start": "astro dev",
    "build": "tinacms build && astro build",
    "postbuild": "pagefind --site dist",
    "preview": "astro preview",
    "lint": "biome lint .",
    "format": "pnpm run format:code && pnpm run format:imports",
    "format:code": "biome format . --write && prettier -w \"**/*\" \"!**/*.{md,mdx}\" --ignore-unknown --cache",
    "format:imports": "biome check --formatter-enabled=false --write",
    "check": "astro check"
  },
  "dependencies": {
    "@astrojs/mdx": "4.2.2",
    "@astrojs/react": "^4.4.0",
    "@astrojs/rss": "4.0.11",
    "@astrojs/sitemap": "3.3.0",
    "@astrojs/starlight": "^0.32.6",
    "@astrojs/tailwind": "6.0.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "astro": "5.5.5",
    "astro-expressive-code": "^0.37.1",
    "astro-icon": "^1.1.5",
    "astro-robots-txt": "^1.0.0",
    "astro-webmanifest": "^1.0.0",
    "cssnano": "^7.1.1",
    "hastscript": "^9.0.1",
    "katex": "^0.16.25",
    "mdast-util-directive": "^3.1.0",
    "mdast-util-to-markdown": "^2.1.2",
    "mdast-util-to-string": "^4.0.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-compiler-runtime": "19.0.0-beta-63e3235-20250105",
    "react-dom": "^18.3.1",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "remark-directive": "^3.0.1",
    "remark-math": "^6.0.0",
    "satori": "0.11.2",
    "satori-html": "^0.3.2",
    "sharp": "^0.33.5",
    "tinacms": "^2.10.0",
    "tldraw": "^3.15.5",
    "tslib": "^2.8.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
